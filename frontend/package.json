    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.21.2",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "date-fns": "^2.9.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-input-color": "^4.0.0",
    "react-redux": "^7.1.3",
    "react-redux-toastr": "^7.6.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-smooth-scrolling": "^1.0.2",
    "react-stomp": "^4.3.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
